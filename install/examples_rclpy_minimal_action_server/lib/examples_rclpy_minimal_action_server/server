#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'examples-rclpy-minimal-action-server==0.9.4','console_scripts','server'
__requires__ = 'examples-rclpy-minimal-action-server==0.9.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('examples-rclpy-minimal-action-server==0.9.4', 'console_scripts', 'server')()
    )
